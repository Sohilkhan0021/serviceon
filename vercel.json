{
  "public": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/serviceon(/.*)?",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/serviceon/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
