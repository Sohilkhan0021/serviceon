{
  "public": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/serviceon",
      "destination": "/serviceon/index.html"
    },
    {
      "source": "/serviceon/:path((?!assets/).*)",
      "destination": "/serviceon/index.html"
    }
  ],
  "headers": [
    {
      "source": "/serviceon/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
